{
  "name": "gravitycar/project",
  "description": "A PHP project with configuration management and exception handling",
  "type": "project",
  "require": {
    "php": "^8.1",
    "doctrine/dbal": "^3.7",
    "monolog/monolog": "^3.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },
  "autoload": {
    "psr-4": {
      "Gravitycar\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gravitycar\\Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}